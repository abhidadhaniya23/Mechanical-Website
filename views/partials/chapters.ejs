<div id="chapter" class="flex flex-col md:flex-row justify-center items-stretch md:items-center flex-wrap py-4 px-5 font-inter">
    <% chapterJsonData.forEach((element,index)=>{ %>
    <a class="px-4 py-3 m-2 md:m-2.5 bg-indigo-200 rounded-sm border-solid border-l-4 border-indigo-500 duration-300 hover:shadow-md" href="/<%=element.chapterId%>/<%= element.id%>">
        <h1><%= index+1 %>. <%=element.chapter%></h1>
    </a>
    <% }) %>
</div>